{
  "presets": [
    "es2015", "stage-0",
    ["env", {
      "modules": false,
      "targets": {
        "browsers": "> 1%",
        "uglify": true
      },
      "useBuiltIns": true
    }]
  ],

  "plugins": [
    "syntax-dynamic-import",
    ["transform-class-properties", { "spec": true }],
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }]
  ]
}
