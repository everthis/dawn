{
  "name": "dawn",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:dev": "webpack --config=client/development.config.js --display-reasons --display-chunks --progress --color --watch",
    "build:new": "webpack --config=client/production.config.js -p",
    "dev-bak": "webpack-dev-server --config client/webpack.dev.config.js --progress --inline --socket shared/sockets/webpack.sock",
    "dev": "webpack-dev-server --config client/webpack.dev.config.js --public 0.0.0.0:8678 --progress --inline --hot --socket .dev-shared/sockets/webpack.sock",
    "clean": "rimraf app/assets/stylesheets/*",
    "prebuild": "npm run clean -s",
    "build": "npm run build:devscripts -s && npm run build:styles -s && npm run build:cp -s",
    "dev:bs": "browser-sync start --proxy localhost:3000 --ws --browser='google chrome canary' --files='assets/*.html'",
    "dev:bs-bak": "browser-sync start --browser='google chrome canary' --server 'assets' --files='app/assets/stylesheets/*.css, assets/*.html, app/assets/javascripts/*.js' --index='index.html'",
    "dev:ruby-moncss": "nodemon --watch front-end/stylesheets --ext scss --exec 'npm run dev:ruby-css' ",
    "dev:moncss": "nodemon --watch front-end/stylesheets --ext scss --exec 'npm run dev:node-css' ",
    "dev:monjs": "nodemon --watch front-end/javascripts --ext js --exec 'npm run dev:js' ",
    "dev:ruby-sass-to-css": "sass --trace --scss --sourcemap=inline --load-path front-end/stylesheets front-end/stylesheets/application.scss .tmp/stylesheets/application.css",
    "dev:add-prefix": "postcss --use autoprefixer --autoprefixer.browsers '> 1%' -m -o app/assets/stylesheets/application.css .tmp/stylesheets/application.css",
    "dev:ruby-css": "npm run dev:ruby-sass-to-css && npm run dev:add-prefix",
    "dev:node-css": "npm run dev:node-sass-to-css && npm run dev:add-prefix",
    "dev:node-sass-to-css": "node-sass --source-map=true --source-map-contents --include-path front-end/stylesheets front-end/stylesheets/application.scss --output .tmp/stylesheets/",
    "prod:node-css": "node-sass --source-map=false --output-style=compressed --include-path front-end/stylesheets front-end/stylesheets/application.scss --output app/assets/stylesheets/",
    "dev:js-bak": "browserify -d -e front-end/javascripts/application.js --outfile app/assets/javascripts/application.js -t babelify ",
    "prod:js": "webpack --debug --devtool inline-source-map --output-pathinfo --module-bind 'js=babel-loader' --resolve-alias 'vue$=vue/dist/vue.common.js' -p --define process.env.NODE_ENV='\"production\"' front-end/javascripts/application.js app/assets/javascripts/application.js",
    "dev:js": "webpack --debug --devtool inline-source-map --output-pathinfo --module-bind 'js=babel-loader' --resolve-alias 'vue$=vue/dist/vue.common.js' front-end/javascripts/application.js app/assets/javascripts/application.js",
    "dev:ruby-build": "npm run dev:ruby-css & npm run dev:js",
    "dev:node-build": "npm run dev:node-css & npm run dev:js",
    "prod:ruby-build": "npm run dev:ruby-css & npm run prod:js",
    "dev:build": "npm run dev:node-css & npm run dev:js",
    "dev:bsmon": "npm run dev:bs & npm run dev:moncss & npm run dev:monjs",
    "dev:ruby-bsmon": "npm run dev:bs & npm run dev:ruby-moncss & npm run dev:monjs",
    "dev:bsmon-slient": "npm run dev:ruby-moncss & npm run dev:monjs",
    "dev:check": "node front-end/npmScripts/mkdir.js",
    "dev:rs": "npm run dev:check && npm run dev:ruby-build && npm run dev:ruby-bsmon",
    "dev:s": "npm run dev:check && npm run dev:build && npm run dev:bsmon",
    "dev:ss": "npm run dev:check && npm run dev:ruby-build && npm run dev:bsmon-slient",
    "dev:node-ss": "npm run dev:check && npm run dev:node-build && npm run dev:bsmon-slient",
    "prod:ss": "npm run dev:check && npm run prod:ruby-build && npm run dev:bsmon-slient"
  },
  "author": "wwwaap <wwwaap@gmail.com> (https://www.everthis.com/)",
  "license": "MIT",
  "dependencies": {
    "babel-preset-es2015": "^6.6.0",
    "codemirror": "^5.23.0",
    "lodash": "^3.0.1",
    "normalize.css": "^4.0.0",
    "spf-dev": "^2.4.8",
    "vue": "^2.1.8"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.4",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "chunk-manifest-webpack-plugin": "^0.1.0",
    "connect-history-api-fallback": "^1.3.0",
    "css-loader": "^0.25.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "http-proxy-middleware": "^0.17.2",
    "imports-loader": "^0.6.5",
    "node-sass": "^3.11.2",
    "postcss-cli": "*",
    "postcss-loader": "^1.1.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^2.2.0",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-manifest-plugin": "^1.1.0",
    "webpack-merge": "^0.19.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  }
}
