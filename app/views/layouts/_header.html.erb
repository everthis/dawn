<header>
    <nav class="nav">
      <ul class="static-links">
        <li class="first-layer"><%= link_to "Homepage", root_path, id: "logo", class: 'spf-link' %></li>
        <% if logged_in? %>
        <li class="first-layer"><%= link_to "Dev", dev_path, class: 'spf-link' %></li>
        <li class="first-layer"><%= link_to "Third party accounts", third_party_accounts_path, class: 'spf-link' %></li>
        <li class="first-layer"><%= link_to "Email-whitelist", email_whitelists_path, class: 'spf-link' %></li>
        <li class="first-layer"><%= link_to "Document", docs_path, class: 'spf-link' %></li>
        <% end %>
      </ul>
      <ul class="user-links">
        <% if logged_in? %>
          <li class="first-layer"><%= link_to "Users", users_path, class: 'spf-link' %></li>
          <li class="first-layer"><%= link_to "Profile", current_user, class: 'spf-link' %></li>
          <li class="first-layer"><%= link_to "Settings", settings_user_path(current_user), class: 'spf-link' %></li>
          <li class="first-layer"><%= link_to "Log out", logout_path, method: "delete" %></li>
        <% else %>
          <li class="first-layer"><%= link_to "Log in", login_path %></li> |
          <li class="first-layer"><%= link_to "Sign up", signup_path %></li>
        <% end %>
      </ul>
    </nav>
</header>