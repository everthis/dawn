<% provide(:title, 'Edit-documentation') %>
<% provide(:name, 'createOrEditDocumentation') %>

<% content_for :head do %>
<style name="createOrEditDocumentation">

</style>
<% end %>

<% content_for :content do %>
<div class="document-container">
    <h1>New Doc</h1>
    <%= render 'form' %>
</div>
<div class="center">
  <%= link_to t('.show'), @doc, class: 'fnpu-link' %> |
  <%= link_to t('.back'), docs_path, class: 'fnpu-link' %>
</div>
<% end %>


<% content_for :foot do %>
  <%= c_javascript_include_tag 'createOrEditDocumentation', name: "createOrEditDocumentation" %>
<% end %>
